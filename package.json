{
  "name": "alexandria-js",
  "version": "0.0.1",
  "description": "Universal library providing constant and polyfilled API for various fanfiction archive websites",
  "repository": "https://github.com/ahlec/alexandria-js.git",
  "author": "Alec Deitloff <alec.deitloff@gmail.com>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc && tsc-alias && fix-esm-import-path build/",
    "test": "jest",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "clean": "rimraf build",
    "format": "prettier --write ."
  },
  "packageManager": "yarn@4.1.1",
  "dependencies": {
    "jsdom": "^24.0.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@ahlec/eslint-plugin": "^0.0.3",
    "@types/eslint": "^8",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fix-esm-import-path": "^1.5.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.1",
    "typescript": "<=5.4.0"
  },
  "prettier": {}
}
