{
  "name": "alexandria-js",
  "version": "0.0.1",
  "description": "Universal library providing constant and polyfilled API for various fanfiction archive websites",
  "repository": "https://github.com/ahlec/alexandria-js.git",
  "author": "Alec Deitloff <alec.deitloff@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc && tsc-alias",
    "test": "jest",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "clean": "rimraf build",
    "format": "prettier --write ."
  },
  "packageManager": "yarn@4.1.1",
  "devDependencies": {
    "@ahlec/eslint-plugin": "^0.0.3",
    "@types/eslint": "^8",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "<=5.4.0"
  },
  "prettier": {}
}
